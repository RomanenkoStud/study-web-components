{
  "name": "@romanenko-dev/simple-timestamp",
  "version": "1.0.7",
  "type": "module",
  "description": "A simple timestamp library for formatting and displaying timestamps.",
  "files": [
    "dist"
  ],
  "main": "./dist/simple-timestamp.js",
  "module": "./dist/simple-timestamp.cjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/simple-timestamp.d.ts"
      ],
      "utils": [
        "dist/utils.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/simple-timestamp.js",
      "require": "./dist/simple-timestamp.cjs"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "publish-patch": "npm version patch && npm publish --access public"
  },
  "author": "RomanenkoStud",
  "license": "ISC",
  "devDependencies": {
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.3",
    "vitest": "^0.34.3"
  },
  "dependencies": {
    "intl-format-cache": "^4.3.1",
    "lit": "^2.8.0"
  }
}
